<h1>todos</h1>
<div>

  <%= render 'form' %>

  <% @items.each do |item| %>
    <ul>
      <li>
        <%= item.compeleted %> | <%= link_to item.title, item %> | <%= link_to 'edit', edit_item_path(item) %> | <%= link_to 'delete', item_path(item), method: :delete, data: {
        confirm: 'Are you sureeeeeee?'
      } %> | <%= link_to 'complete', "/items/#{item.id}/complete", method: :put %></li>
    </ul>
  <% end %>

</div>
